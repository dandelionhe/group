<div class="col-md-12">
  <% if current_user && current_user == @group.user %>
  <aside class="col-md-2 pull-right">
    <%= link_to "Edit",edit_group_path(@group),class: "btn btn-primary" %>
  </aside>
  <% end %>
  <div class="col-md-10 well">
    <h2><%= @group.title %></h2>
    <p><%= @group.description %></p>
  </div>

</div>
